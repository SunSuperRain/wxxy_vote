<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sun.mapper.CollegeMapper">

    <select id="getColleges" resultType="com.sun.entity.College">
        select * from college;
    </select>

    <select id="findCollegeNO" parameterType="Integer" resultType="String">
        select * from college where collegeNo = #{collegeNo};
    </select>
</mapper>